    "from Bio.Seq import Seq\n",
    "\n",
    "file = open(\"rosalind_hamm.txt\", \"r\")\n",
    "sequences = []\n",
    "distance = 0\n",
    "for i, seq in enumerate(file):\n",
    "    sequences.append(seq)\n",
    "for j in range(len(sequences[0])):\n",
    "    if (sequences[0][j] != sequences[1][j]):\n",
    "        distance += 1\n",
    "print(distance)"
